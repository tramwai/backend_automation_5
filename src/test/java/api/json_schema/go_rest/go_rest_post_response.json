{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
    "id",
    "name",
    "email",
    "gender",
    "status"
  ],
  "properties": {
    "id": {
      "type": "integer",
      "default": 0,
      "title": "The id Schema",
      "examples": [
        900541
      ]
    },
    "name": {
      "type": "string",
      "default": "",
      "title": "The name Schema",
      "examples": [
        "Amanda Botsford II"
      ]
    },
    "email": {
      "type": "string",
      "default": "",
      "title": "The email Schema",
      "examples": [
        "Drew.Schaefer@gmail.com"
      ]
    },
    "gender": {
      "type": "string",
      "default": "",
      "title": "The gender Schema",
      "examples": [
        "male"
      ]
    },
    "status": {
      "type": "string",
      "default": "",
      "title": "The status Schema",
      "examples": [
        "active"
      ]
    }
  },
  "examples": [{
    "id": 900541,
    "name": "Amanda Botsford II",
    "email": "Drew.Schaefer@gmail.com",
    "gender": "male",
    "status": "active"
  }]
}